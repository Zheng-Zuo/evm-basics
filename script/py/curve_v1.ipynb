   "source": [
    "# Equations from v1 whitepaper\n",
    "x, y, D, A = symbols(\"x y D A\")\n",
    "n = 2\n",
    "f = A*D*n**n + D**(n+1) / (n**n * x*y) - A*n**n * (x + y) - D\n",
    "\n",
    "display(f)\n",
    "\n",
    "df_dD = diff(f, D)\n",
    "print(df_dD)"
   ]
